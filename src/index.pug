doctype html
html
  head
    include modules/head.pug
  body
    include modules/noscript.pug
    script(type='text/javascript', src='js/queryloader2.min.js')
    script(type='text/javascript', id='__bs_script__')
      //<! [CDATA[document.write("<script async src='http://HOST:3000/browser-sync/browser-sync-client.js?v=2.26.7'><\/script>".replace("HOST", location.hostname)); //]]>
    div.container-fluid
      div.row
        div.main.well.col-xs-12.col-md-8.col-md-offset-2
          //- Header
          include modules/header.pug

          //- Responsive Web Design
          div.row(title='freeCodeCamp Responsive Web Design Certification: acquired!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleRWD') Responsive Web Design
              i.fa.fa-mobile
              p.description html &#8594; css &#8594; visual design &#8594; accessibility
          div.row.collapse#toggleRWD
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //- RWD sites (dynamic)
              each val in responsive.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Responsive Web Design Certificate
              a(href='https://www.freecodecamp.org/certification/charmedsatyr/responsive-web-design' target='_blank' rel='noopener noreferrer')
                p.cert Responsive Web Design Certificate
          br

          //- JavaScript Algorithms and Data Structures
          div.row(title='freeCodeCamp JavaScript Algorithms and Data Structures Certification: acquired!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleDSA') JavaScript Algorithms and Data Structures
              i.fa.fa-code
              p.description scripting challenges &#8594; algorithms &#8594; small programs
          div.row.collapse#toggleDSA
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //- DSA algorithms (Gists)
              script(src="https://gist.github.com/CharmedSatyr/be02429bb75c4b140945b6f0858c3901.js")
              //- JavaScript Algorithms and Data Structures Certificate
              a(href='https://www.freecodecamp.org/certification/charmedsatyr/javascript-algorithms-and-data-structures' target='_blank' rel='noopener noreferrer')
                p.cert JavaScript Algorithms and Data Structures Certificate
          br

          //- Front End Libraries
          div.row(title='freeCodeCamp Front End Libraries Certification: acquired!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleFEL') Front End Libraries
              i.fa.fa-book
              p.description bootstrap &#8594; sass &#8594; jQuery &#8594; react &#8594; redux
          div.row.collapse#toggleFEL
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //- Libraries sites (dynamic)
