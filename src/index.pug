                p.cert APIs and Microservices Certificate
          br

          //- Information Security and Quality Assurance
          div.row(title='freeCodeCamp Information Security and Quality Assurance Certification: in progress!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleISQA') Information Security and Quality Assurance
              i.fa.fa-lock
              p.description content security policy &#8594; xss filtering &#8594; http strict transport security
          div.row.collapse#toggleISQA
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //- Information Security and Quality Assurance sites (dynamic)
              each val in infosec.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Information Security and Quality Assurance Certificate
              p.cert Information Security and Quality Assurance Certificate in progress!
          br

          //- Legacy Front End
          div.row(title='freeCodeCamp Legacy Front End Certification: acquired!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleLFE') Legacy Front End
              i.fa.fa-free-code-camp
              p.description basic &#8594; intermediate &#8594; advanced
          div.row.collapse#toggleLFE
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //-  Legacy Front End sites (dynamic)
              each val in legacy_front.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Legacy Front End Development Certificate
              a(href='https://www.freecodecamp.org/certification/charmedsatyr/legacy-front-end' target='_blank' rel='noopener noreferrer')
                p.cert Legacy Front End Development Certificate
          br

          //- Legacy Back End
          div.row(title='the full stack experience!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleLBE') Legacy Back End
              i.fa.fa-free-code-camp
              p.description node &#8594; express &#8594; mongodb
          div.row.collapse#toggleLBE
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //-  Legacy Back End sites (dynamic)
              each val in legacy_back.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Legacy Back End Development Certificate
              a(href='https://www.freecodecamp.org/certification/charmedsatyr/legacy-back-end' target='_blank' rel='noopener noreferrer')
                p.cert Legacy Back End Development Certificate
          br

          //- Legacy Data Visualization
          div.row(title='freeCodeCamp Legacy Data Visualization Certification: obsolete!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleLDV') Legacy Data Visualization
              i.fa.fa-free-code-camp
              p.description react &#8594; d3
          div.row.collapse#toggleLDV
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //-  Legacy Data Visualization sites (dynamic)
              each val in legacy_data.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Legacy Data Visualization Certificate
              p.cert This certificate is not offered in the new curriculum
          br

          // Other Projects
          include modules/other-projects.pug

          // Donate
          include modules/donate.pug

          // Contact
          include modules/contact.pug

          //Social sites
          include modules/social.pug
          
    script(type='text/javascript', src='js/jquery-3.1.0.min.js')
    script(type='text/javascript', src='js/bootstrap.min.js')
    script(type='text/javascript', src='js/index.js')
    script(type='text/javascript', src='js/contact.js')
