                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Front End Libraries Certificate
              a(href='https://www.freecodecamp.org/certification/charmedsatyr/front-end-libraries' target='_blank' rel='noopener noreferrer')
                p.cert Front End Libraries Certificate
          br

          //- Data Visualization
          div.row(title='check out my progress!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleDV') Data Visualization
              i.fa.fa-camera-retro
              p.description svg &#8594; d3
          div.row.collapse#toggleDV
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
              br
              //- Data visualization sites (dynamic)
              each val in visualization.sites
                a#fader(href=`${val.url}`)
                  div.thumbnail-container(title=`${val.title}`)
                    div.thumbnail
                      iframe(src=`${val.url}` frameborder='0' onload='this.style.opacity=1')
                  p !{val.title}
                br
              //- Data Visualization Certificate
              p.cert Data Visualization Certificate in progress!
          br

          //- APIs and Microservices
          div.row(title='freeCodeCamp APIs and Microservices Certification: acquired!')
            div.headings.btn.btn-primary.col-xs-12.col-md-8.col-md-offset-2(data-toggle='collapse', data-target='#toggleAPIs') APIs and Microservices
              i.fa.fa-server
              p.description basic &#8594; intermediate &#8594; advanced
          div.row.collapse#toggleAPIs
            div.inner.well.col-xs-12.col-md-8.col-md-offset-2
